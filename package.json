  "license": "MIT",
  "dependencies": {
    "normalize.css": "^3.0.3",
    "perfect-scrollbar": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "browser-sync": "^2.9.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-css": "^3.4.3",
    "css-loader": "^0.28.11",
    "postcss-cli": "^2.1.0",
    "postcss-import": "^7.0.0",
    "postcss-loader": "^2.1.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  }
